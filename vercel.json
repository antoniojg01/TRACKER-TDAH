{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "name": "cronos-time-evolution",
  "alias": ["cronos"],
  
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  
  "rewrites": [
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ],
  
  "redirects": [
    {
      "source": "/home",
      "destination": "/",
      "permanent": true
    }
  ],
  
  
  "trailingSlash": false,
  "cleanUrls": true,
  
  "env": {
    "VITE_FIREBASE_API_KEY": "AIzaSyDnQlzkoxsZ5bZhlWASgAnBXtHP3-Occcg",
    "VITE_FIREBASE_AUTH_DOMAIN": "controle-de-assinaturas.firebaseapp.com",
    "VITE_FIREBASE_PROJECT_ID": "controle-de-assinaturas",
    "VITE_FIREBASE_STORAGE_BUCKET": "controle-de-assinaturas.firebasestorage.app",
    "VITE_FIREBASE_MESSAGING_SENDER_ID": "421292619235",
    "VITE_FIREBASE_APP_ID": "1:421292619235:web:7a2ee16000cb309cc91cd6"
  },
  
  "regions": ["iad1"],
  
  
  "github": {
    "enabled": true,
    "autoAlias": true,
    "silent": false,
    "autoJobCancelation": true
  },
  
  "git": {
    "deploymentEnabled": {
      "main": true,
      "master": true
    }
  },
  
  "crons": []
}
